{
  "buildCommand": "npm run build",
  "framework": "vite",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets|favicon).*)",
      "destination": "/index.html"
    }
  ]
}
